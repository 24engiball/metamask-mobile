{
  "extends": ["tslint-config-standard", "tslint-react"],
  "rules": {
    "jsx-alignment": false,
    "jsx-boolean-value": [true, "never"],
    "jsx-no-multiline-js": false,
    "jsx-wrap-multiline": false,
    "no-empty": false,
    "no-unused-variable": false,
    "semicolon": false,
    "ter-indent": false
  }
}
